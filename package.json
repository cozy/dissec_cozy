{
  "name": "dissec_cozy",
  "version": "0.1.0",
  "scripts": {
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "cs lint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "start": "cs start --browser",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/JulienMirval/dissec_cozy.git}",
    "test": "COZY_URL=cozy.localhost:8080 COZY_CREDENTIALS=cozy cs test --verbose --coverage --testPathIgnorePatterns integration",
    "test:integration": "cs test --verbose --coverage --testPathPattern integration",
    "cozyPublish": "git fetch origin ${DEPLOY_BRANCH:-build}:${DEPLOY_BRANCH:-build} && cs publish --token $REGISTRY_TOKEN --build-commit $(git rev-parse ${DEPLOY_BRANCH:-build})",
    "stack:docker:dev": "cs stackDocker",
    "stack:docker:prod": "cs stackDocker --prod",
    "dissecozy:aggregation": "yarn run cozy-konnector-dev -m manifest.webapp build/services/aggregation/dissecozy.js",
    "dissecozy:categorize": "yarn run cozy-konnector-dev -m manifest.webapp build/services/categorize/dissecozy.js",
    "dissecozy:contribution": "yarn run cozy-konnector-dev -m manifest.webapp build/services/contribution/dissecozy.js",
    "test:centralized-vs-dissec": "bash ./scripts/testCentralizedVsDissec.sh",
    "populate": "node ./scripts/cli/populate.js",
    "populate:centralized": "node ./scripts/cli/populate.js --centralized",
    "measure": "node ./scripts/localVsDissecLearning.js",
    "categorize": "node ./scripts/cli/categorize.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulienMirval/dissec_cozy.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/JulienMirval/dissec_cozy/issues"
  },
  "imports": {
    "/": "src"
  },
  "homepage": "https://github.com/JulienMirval/dissec_cozy#readme",
  "devDependencies": {
    "babel-preset-cozy-app": "^2.0.2",
    "cozy-ach": "^1.48.0",
    "cozy-jobs-cli": "1.10.4",
    "cozy-scripts": "6.3.11",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "git-directory-deploy": "1.5.1",
    "react-hot-loader": "4.6.5",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "1.5.3",
    "stylint": "1.5.9"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@tensorflow-models/toxicity": "^1.2.2",
    "@tensorflow/tfjs": "^3.7.0",
    "btoa": "^1.2.1",
    "classificator": "^0.3.4",
    "clsx": "^1.1.1",
    "commander": "^10.0.0",
    "cozy-bar": "7.12.3",
    "cozy-client": "35.3.1",
    "cozy-doctypes": "^1.82.2",
    "cozy-flags": "^2.7.2",
    "cozy-konnector-libs": "5.2.1",
    "cozy-logger": "^1.7.0",
    "cozy-realtime": "^4.4.1",
    "cozy-ui": "37.8.0",
    "d3": "^7.8.4",
    "d3-drag": "^3.0.0",
    "lzutf8": "^0.6.0",
    "node-fetch": "^2.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "^4.4.2",
    "uuid": "^8.3.2"
  }
}
